service fastagi
{
    socket_type     = stream
    user            = root
    group           = root
    server          = /var/lib/asterisk/agi-bin/lib/phpagi/phpagi-fastagi.php
    wait            = no
    protocol        = tcp
    bind            = 127.0.0.1
    disable         = no
    log_type        = SYSLOG authpriv
    log_on_success  = HOST PID
    log_on_failure  = HOST
    cps             = 150 30    
    nice            = 10
}
